---
import BaseLayout from '../layouts/BaseLayout.astro';

const pages = [
  { url: '/moon', name: 'Moon' },
  { url: '/blending', name: 'Blending' },
  { url: '/cats', name: 'Cats' },
];
---

<BaseLayout title="Demos">
  <h1>Demos</h1>

  <ul>
    {
      pages.map((page) => (
        <li>
          <a href={page.url}>{page.name}</a>
        </li>
      ))
    }
  </ul>
</BaseLayout>
